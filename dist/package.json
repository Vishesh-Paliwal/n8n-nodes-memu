{
    "name": "n8n-nodes-memu",
    "version": "1.0.0",
    "description": "n8n community nodes for MemU - integrate agentic memory capabilities into your n8n workflows. Memorize, retrieve, and manage multimodal memory data from conversations, documents, images, videos, and audio.",
    "keywords": [
        "n8n-community-node-package",
        "n8n",
        "memu",
        "memory",
        "ai",
        "agent",
        "llm",
        "rag",
        "retrieval",
        "multimodal",
        "automation",
        "workflow"
    ],
    "license": "MIT",
    "homepage": "https://github.com/Vishesh-Paliwal/n8n-nodes-memu#readme",
    "author": {
        "name": "MemU Team",
        "email": "support@memu.so"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/Vishesh-Paliwal/n8n-nodes-memu.git"
    },
    "bugs": {
        "url": "https://github.com/Vishesh-Paliwal/n8n-nodes-memu/issues"
    },
    "engines": {
        "node": ">=18.10",
        "pnpm": ">=9.0"
    },
    "packageManager": "pnpm@9.1.0",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "scripts": {
        "preinstall": "npx only-allow pnpm",
        "build": "tsc && gulp build:icons",
        "build:prod": "npm run clean && npm run build",
        "clean": "rimraf dist",
        "dev": "tsc --watch",
        "format": "prettier nodes credentials --write",
        "format:check": "prettier nodes credentials --check",
        "lint": "eslint nodes/**/*.ts credentials/**/*.ts",
        "lintfix": "eslint nodes/**/*.ts credentials/**/*.ts --fix",
        "typecheck": "tsc --noEmit",
        "prepublishOnly": "npm run build:prod && eslint -c .eslintrc.prepublish.js nodes credentials package.json --ext .ts,.json",
        "test": "echo \"No tests configured yet\" && exit 0"
    },
    "files": [
        "dist"
    ],
    "n8n": {
        "n8nNodesApiVersion": 1,
        "credentials": [
            "dist/credentials/MemUCloudApi.credentials.js"
        ],
        "nodes": [
            "dist/nodes/MemU/MemUMemorize.node.js",
            "dist/nodes/MemU/MemURetrieve.node.js",
            "dist/nodes/MemU/MemUListCategories.node.js"
        ]
    },
    "devDependencies": {
        "@types/node": "^18.16.16",
        "@typescript-eslint/parser": "^5.59.8",
        "eslint": "^8.42.0",
        "eslint-plugin-n8n-nodes-base": "^1.11.0",
        "gulp": "^4.0.2",
        "n8n-workflow": "^1.2.0",
        "prettier": "^2.8.8",
        "rimraf": "^5.0.0",
        "typescript": "^5.1.3"
    },
    "dependencies": {
        "axios": "^1.4.0"
    },
    "peerDependencies": {
        "n8n-workflow": "*"
    }
}
